//Given an array of positive integers where all numbers occur even number of times except one number which occurs odd number of times. Find the number.


using namespace std;
int main()
 {
	//code
	int t;
	cin>>t;
	while(t--)
	{
	    int n,i,ans=0;
	    cin>>n;
	    int a[n];
	    for(i=0;i<n;i++)
	    cin>>a[i];
	    for(i=0;i<n;i++)
	    ans=ans^a[i];
	    cout<<ans<<endl;
	    
	}
	return 0;
}
